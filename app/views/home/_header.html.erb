<div class="wrapper">
  <div class="row">
  <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1">
        <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <%= image_tag 'gingerApps.jpg' %>
        <div class="carousel-caption">
        </div>
      </div>
      <div class="item">
        <%= image_tag 'DrawCollab.jpg' %>
        <div class="carousel-caption">
        </div>
      </div>
      <div class="item">
        <%= image_tag 'Spheres.jpg' %>
        <div class="carousel-caption">
        </div>
      </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
      <span></span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
      <span></span>
    </a>
  </div>

  <nav class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <%= image_tag 'hamburger-icon-yellow.svg', size: "16x16" %>
        </button>
        <div id="logotype" class="navbar-left">
          <%=link_to (image_tag("gingerIcon.png", class: "img_logo")), root_url %>
        </div>
      </div>
      <div class="collapse navbar-collapse navbar-right" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li><%= link_to 'App portfolio', projects_path, :class => "button navbar-button" %></li>
          <li><%= link_to 'Contact', contact_path, :class => "button navbar-button" %></li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
</div>
